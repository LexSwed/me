---
import type { ComponentProps } from "astro/types";
import LogoImage from "../../public/logo-inner.svg";

export type Props = ComponentProps<typeof LogoImage>;

const { "class": className, "class:list": classList, ...props } = Astro.props;
---

<LogoImage
  aria-hidden="true"
  class:list={[
    classList,
    "text-on-background bg-on-primary h-10 w-10 rounded-xl p-2",
    className,
  ]}
  {...props}
/>
